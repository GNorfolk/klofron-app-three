{
  "private": true,
  "scripts": {
    "dev": "clear && next dev",
    "clean": "rm -rf .next .serverless .serverless_nextjs node_modules tf/.terraform tf/.terraform.lock.hcl tf/ka3-nextjs.zip next-env.d.ts",
    "build": "next build && npx --package @sladg/nextjs-lambda cli pack && unzip next.out/assetsLayer.zip -d next.out/assets-layer",
    "copy": "cp -R .next/serverless/ .serverless_nextjs/default-lambda/ && cp .env .serverless_nextjs/default-lambda/.env.production.local",
    "terraform": "terraform -chdir=tf/deploy init && terraform -chdir=tf/deploy apply",
    "sync": "aws s3 sync --acl private .serverless_nextjs/assets/ s3://ka3-nextjs-app/",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E1AACFG4H0AQC2 --invalidation-batch \"{ \\\"Paths\\\": { \\\"Quantity\\\": 2, \\\"Items\\\": [ \\\"/\\\", \\\"/*\\\" ] }, \\\"CallerReference\\\": \\\"%RANDOM%\\\" }\"",
    "deploy": "npm run clean && npm install && npm run build && npm run terraform && npm run invalidate"
  },
  "dependencies": {
    "@next/font": "^14.2.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^4.29.19",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "constructs": "^10.0.0",
    "date-fns": "^2.30.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.390.0",
    "next": "^12.3.0",
    "next-auth": "4.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.3",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "serverless": "^1.74.1",
    "source-map-support": "^0.5.21",
    "swr": "^2.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@types/jest": "^29.5.4",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "autoprefixer": "^10.4.19",
    "jest": "^29.6.4",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  }
}
