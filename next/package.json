{
  "private": true,
  "scripts": {
    "dev": "clear && next dev",
    "clean": "rm -rf .next .open-next .serverless_nextjs node_modules tf/deploy/.terraform tf/deploy/.terraform.lock.hcl tf/deploy/ka3-nextjs.zip next-env.d.ts",
    "build": "next build",
    "build:open-next": "npx open-next@2.3.9 build",
    "terraform": "terraform -chdir=tf/deploy init && terraform -chdir=tf/deploy apply -auto-approve",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E7MMBC8I0ONGT --invalidation-batch \"{ \\\"Paths\\\": { \\\"Quantity\\\": 2, \\\"Items\\\": [ \\\"/\\\", \\\"/*\\\" ] }, \\\"CallerReference\\\": \\\"%RANDOM%\\\" }\"",
    "deploy": "npm run clean && npm install && npm run build:open-next && npm run terraform && npm run invalidate",
    "deploy:quick": "npm run build:open-next && npm run terraform && npm run invalidate"
  },
